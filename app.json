{
  "expo": {
    "name": "travel-offline",
    "slug": "travel-offline",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "windowSoftInputMode": "adjustPan",
        "package": "com.shellyapps.dateradar",
        "googleMapsApiKey": "AIzaSyAbC71b9HV0oXLVseLllK78gdXjBecuCDA"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Date Radar to use your location."
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
